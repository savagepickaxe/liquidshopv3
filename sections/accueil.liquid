<section>

<div class="three-images-wrapper">

{% if section.settings.image != blank %}
    <div class="one-user">
        <h1></h1>
    </div>
{% else %}
<div class="one">
    <h1></h1>
</div>
{% endif %}

{% if section.settings.image2 != blank %}
    <div class="two-user">
        <h1></h1>
    </div>
{% else %}
<div class="two">
    <h1></h1>
</div>
{% endif %}

{% if section.settings.image3 != blank %}
    <div class="three-user">
        <h1></h1>
    </div>
{% else %}
<div class="three">
    <h1></h1>
</div>
{% endif %}
</div>

</section>

{% schema %}
    {
      "name": "three-images",
      "settings": [
        {
          "type": "image_picker",
          "id": "image",
          "label": "image",
          "info": "400 x 120px .png recommended"
        },
        {
          "type": "image_picker",
          "id": "image2",
          "label": "image2",
          "info": "400 x 120px .png recommended"
        },
        {
          "type": "image_picker",
          "id": "image3",
          "label": "image3",
          "info": "400 x 120px .png recommended"
        }
      ],
      "blocks": [],
      "presets": [
        {
          "name": "accueil"
        }
      ]
    }
    {% endschema %}